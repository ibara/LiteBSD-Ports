	$(AM_V_at)$(am__mv) $(DEPDIR)/loadavg-getloadavg.Tpo $(DEPDIR)/loadavg-getloadavg.Po
#	$(AM_V_CC)source='getloadavg.c' object='loadavg-getloadavg.o' libtool=no \
